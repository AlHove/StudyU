<?xml version="1.0" encoding="UTF-8"?>

<LinearLayout android:layout_height="match_parent" android:layout_width="match_parent" android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content">

        <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/examName" android:textColor="#0022ff" android:padding="10dp" android:text="Empty File"/>

    </LinearLayout>
    <LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content">
    <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/examDate" android:textColor="#0022ff" android:text="@string/nodata" android:paddingLeft="20dp"/>
    <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/examTime" android:textColor="#0022ff" android:padding="10dp" android:text="@string/nodata"/>
</LinearLayout>
</LinearLayout>